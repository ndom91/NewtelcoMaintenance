{
  "name": "Newtelco Maintenance Mail Add-er",
  "description": "Newtelco GmbH - Add Maintenance Mail to WebApp directly from Gmail",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": ["inboxsdk.js","content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "manifest_version": 2,
  "icons": {
    "32": "/images/done2.png"
  }
}
